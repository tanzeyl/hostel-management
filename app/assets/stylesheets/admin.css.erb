.Table {
  background-image: url(<%= asset_data_uri 'image3.jpg' %>);
  background-size: cover;
  background-repeat: no-repeat;
  max-height: 100%;
  min-width: 1200px;
  max-width: 100%;
  margin: 0 auto;
  color: white;
  font-size: 25px;
  font-family: 'Cormorant Garamond', serif;
  border-radius: 4px;
}

.Table table, th, td {
  border: 3px solid white;
  border-collapse: collapse;
  padding: 10px;
}

.KitchenButtons {
  max-width: 200px;
  margin: 10px auto;
  background-color: rgba(0, 0, 0, 0.6);
  padding: 10px;
}

.DeleteButton {
  background-color: rgba(255, 0, 0, 0.3);
  padding: 8px;
  color: white;
  font-size: 25px;
  border: transparent;
}

.TableButton {
  margin-top: 2%;
}

.NewHostelBG {
  background-image: url(<%= asset_data_uri 'image4.jpg' %>);
  background-size: cover;
  background-repeat: no-repeat;
  max-height: 100%;
}
