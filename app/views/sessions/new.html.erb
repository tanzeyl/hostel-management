<br><br><br><br><br>
<center class="SignUp-Form">
  <h1 class="class="NewUsers-title"">Please enter these details.</h1>
  <section class="UserNew">
    <%= form_with url: sessions_path do |form| %>
      <div class="Form-Section">
        <p class="FormInput-label">Email:</p>
        <%= form.email_field :email, class: "Form-Input" %>
      </div>
      <div class="Form-Section">
        <p class="FormInput-label">Password:</p>
        <%= form.password_field :password, class: "Form-Input" %>
      </div>
      <div class="Form-Section">
      <%= form.submit "Log In!", class: "Form-Submit" %>
      </div>
    <% end %>
  </section>
</center>
